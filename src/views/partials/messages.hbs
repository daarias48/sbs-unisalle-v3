{{#if successMsg}}
    <div class="alert alert-success alert-dismissible fade show d-flex justify-content-between" role="alert">
        {{successMsg}}
        <button type="button" class="btn-close" style="border: transparent; background-color: transparent;" data-bs-dismiss="alert" aria-label="Close"><i class="fas fa-times-circle"></i></button>
    </div>
{{/if}}